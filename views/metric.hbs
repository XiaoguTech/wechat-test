{{!-- 显示图表 --}}
  <style type="text/css">
  body {
    background-color: #FFFFFF;
  }
  .ui.menu .item img.logo {
    margin-right: 1.5em;
  }
  .main.container {
    margin-top: 7em;
  }
  .wireframe {
    margin-top: 2em;
  }
  .ui.footer.segment {
    margin: 5em 0em 0em;
    padding: 5em 0em;
  }
  </style>
{{!-- 展示默认图表以及result中的对应图表 --}}
{{#if result}}
  <div class="ui main text container">
    <h1 class="ui header">默认1</h1>
    <iframe src="http://grafana.xiaogu-tech.com:3000/dashboard-solo/db/new-dashboard-copy?orgId=2&panelId=1">
    </iframe>
    <h1 class="ui header">默认2</h1>
    <iframe src="http://grafana.xiaogu-tech.com:3000/dashboard-solo/db/new-dashboard-copy?orgId=2&panelId=1">
    </iframe>
    {{!-- result中所有的内容 --}}
    {{#each result}}
      <h1 class="ui header">{{this.name}}</h1>
      <iframe src={{this.url}}></iframe>
    {{/each}}
  </div>
{{else}}
{{!-- 展示默认图表 --}}
  <div class="ui main text container">
    <h1 class="ui header">默认1</h1>
    <iframe src="http://grafana.xiaogu-tech.com:3000/dashboard-solo/db/new-dashboard-copy?orgId=2&panelId=1">
    </iframe>

    <h1 class="ui header">默认2</h1>
    <iframe src="http://grafana.xiaogu-tech.com:3000/dashboard-solo/db/new-dashboard-copy?orgId=2&panelId=1">
    </iframe>
  </div>
{{/if}}







